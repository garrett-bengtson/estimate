<html xmlns:t="http://tapestry.apache.org/schema/tapestry_5_4.xsd">
   <head>
       <title>Add Hours</title>
   </head>
   <body>
       <t:form t:id="hoursForm">
           <t:errors />
       <!-- timeStamp Input Field -->
       <t:label for="timeStamp">TimeStamp</t:label>
       <span class="input-group-text" title="TimeStamp">
           <i class="FA fa-calendar"></i>
       </span>
       <t:dateField t:id="timeStamp" value="timeStampDate" class="form-control" placeholder="MM/DD/YYYY"/>
     
      
       <!-- Hours Logged Field -->
       <t:label for="hoursLogged">Hours Logged</t:label>
       <t:textField t:id="hoursLogged" value="hour.hoursLogged" />
      
       <input type="submit" value="Add Hours" />
       </t:form>
   </body>
</html>